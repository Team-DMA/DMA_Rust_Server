          {
            "TextLines": [
              "<size=5> </size>",
              "<color=#75c9eb><color=#75c9eb><size=17><b>NTeleportation:</b></size></color></color>",
              "/sethome – Saves your current position as home",
              "/removehome – Removes the location home.",
              "/home add [NAME] - Saves your current position as the location NAME.",
              "/home list - Shows you a list of all the locations you have saved.",
              "/home remove [NAME] - Removes the location NAME from your saved homes.",
              "/home [NAME] - Teleports you to the home location.",
              "/tpr [NAME OF PLAYER] - Sends a teleport request to the player.",
              "/tpa - Accepts an incoming teleport request.",
              "/tpc - Cancel teleport or request.",
              "/tpinfo - Shows limits and cooldowns.",
              "/tphelp - Shows help.",
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>Kits:</b></size></color>",
              "/kit – Shows the Kits available.",
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>Skins:</b></size></color>",
              "/skin – open skin menu",
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>Gestures:</b></size></color>",
              "/gestures - Opens up the Gestures-UI"
            ],
            "ImageSettings": []
          },
          {
            "TextLines": [
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>BGrade:</b></size></color>",
              "/bgrade 0 - Disable BGrade",
              "/bgrade 1 - Enable automatic Wood upgrades",
              "/bgrade 2 - Enable automatic Stone upgrades",
              "/bgrade 3 - Enable automatic Metal upgrades",
              "/bgrade 4 - Enable automatic Armored upgrades",
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>Building Grades:</b></size></color>",
              "/up [grade] (e.g. 1 for wood upgrade) - Upgrade each block which is attached",
              "to the building you are looking at by one grade or to selected grade",
              "/down [grade] (e.g. 1 for wood downgrade) - Downgrade each block which is attached",
              "to the building you are looking at by one grade or to selected grade",
              "<size=5> </size>",
              "<color=#75c9eb><size=17><b>Clans:</b></size></color>",
